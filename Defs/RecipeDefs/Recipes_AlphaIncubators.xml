<Defs>


	<RecipeDef>
		<defName>CreateAlphaBearIncubator</defName>
		<label>alpha bear incubator</label>
		<description>Fill an incubator with alpha genes from a bear</description>
		<jobString>Filling Incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>
			<li>AnimalEnrichmentCenter</li>	
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>EmptyIncubator</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>BearSerum</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>OrganicPulp</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>EmptyIncubator</li>
				<li>BearSerum</li>
				<li>OrganicPulp</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<AlphaBearIncubator>1</AlphaBearIncubator>
		</products>
		<skillRequirements>
			<Intellectual>5</Intellectual>
		</skillRequirements>
	</RecipeDef>

