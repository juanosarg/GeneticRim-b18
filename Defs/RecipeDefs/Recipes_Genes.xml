<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	
	
	<RecipeDef>
		<defName>BaseIncubator</defName>
		<label>manufacture empty incubator</label>
		<description>Manufacture an empty incubator using Steel and an organic growth compound.</description>
		<jobString>Manufacturing empty incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>800</workAmount>
		<recipeUsers>
				<li>GeneticExtractionTable</li>	  
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
				
					<thingDefs>
						<li>Steel</li>
					
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
				
					<thingDefs>
						<li>OrganicPulp</li>
						
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
		
			<thingDefs>
				<li>Steel</li>
				<li>OrganicPulp</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<EmptyIncubator>1</EmptyIncubator>
		</products>
		
	</RecipeDef>
	
