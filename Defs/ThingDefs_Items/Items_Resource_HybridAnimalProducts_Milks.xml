<Defs>



	<ThingDef ParentName="OrganicProductBase">
		<defName>GR_WeirdMilk</defName>
		<label>weird milk</label>
		<description>This strange milk has a sour, chemical taste.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Milk</texPath>
			<color>(186,149,0)</color>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MarketValue>3.1</MarketValue>
			<Mass>0.045</Mass>
			<MaxHitPoints>60</MaxHitPoints>
		</statBases>
		<tradeability>Sellable</tradeability>
		<comps>
			<li Class="CompProperties_FoodPoisoningChance">
				<humanlikeOnly>true</humanlikeOnly>
			</li>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>14</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<preferability>RawTasty</preferability>
			<foodType>AnimalProduct, Fluid</foodType>
			<nutrition>0.04</nutrition>
		</ingestible>
		<thingCategories>
			<li>AnimalProductRaw</li>
		</thingCategories>
	</ThingDef>
	
